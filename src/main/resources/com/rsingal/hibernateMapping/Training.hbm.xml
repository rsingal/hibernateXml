<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.rsingal.entity.Training" table="TRAINING">
		<id name="trainingId" type="integer" column="ID_TRAINING">
			<generator class="increment"></generator>
		</id>
		<property name="trainingName" type="string">
			<column name="TRAINING_NAME" length="32" />
		</property>
	</class>
</hibernate-mapping>